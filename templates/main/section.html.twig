{% extends 'template.html.twig' %}

{% block title %}{{ parent() }} | {{ section.title }}{% endblock %}

{% block content %}

        <!-- Main Content-->
        <div class="container px-4 px-lg-5">
            <div class="row gx-4 gx-lg-5 justify-content-center">
                <div class="col-md-10 col-lg-8 col-xl-9">
                    <!-- Post preview-->
                    <div class="post-preview">

                            <h2 class="post-title">Thèmes pour débat ou dissertation sur l'univers, la pensée, la mort,...</h2>
                        <hr>
                        <h3 class="post-subtitle">{{ section.title }}</h3>
                        <hr>



                        <p class="post-meta">
                            Posté par Charles J. Pitz
                        </p>
                    </div>
                    <!-- Divider-->


            </div>
        </div>

{% endblock %}
